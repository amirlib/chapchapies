{
	"compileOnSave": false,
	"compilerOptions": {
		"baseUrl": ".",
		"declaration": false,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"importHelpers": true,
		"lib": ["es2023"],
		"module": "esnext",
		"moduleResolution": "bundler",
		"paths": {},
		"rootDir": ".",
		"skipDefaultLibCheck": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"target": "esnext"
	},
	"exclude": [
		"node_modules",
		"tmp"
	]
}
